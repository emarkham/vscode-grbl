{
    "name": "grbl",
    "displayName": "GrblSyntaxHighlighting",
    "description": "Grbl Syntax Highlighting",
    "version": "0.0.1",
    "publisher": "emarkham",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "grbl",
            "aliases": ["Grbl"],
            "extensions": [".nc",".ngc",".prg",".mpf",".gcode"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "grbl",
            "scopeName": "source.grbl",
            "path": "./syntaxes/GrblSyntaxHighlightingDef.tmLanguage"
        }]
    }
}
